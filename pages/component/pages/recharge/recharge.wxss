/* pages/recharge/recharge.wxss */
.moneylist{
  /* margin-top: 16px; */
  clear: both;
}
.moneylist .moneyitem{
  float: left;
  margin-right: 10%;
  margin-bottom: 16px;
  width: 45%;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  box-sizing: border-box;
  border-radius: 6px;
  border: 1px solid #43CD80;
}
.moneylist .moneyitem.active{
  background-color:#43CD80;
  color: #fff;
}
.moneylist .moneyitem:nth-child(2n){
    margin-right: 0;
}
.moneylist .moneyitem:nth-child(2n-1).moneyitem:last-child{
  width: 100%;
  margin-right: 0;
}